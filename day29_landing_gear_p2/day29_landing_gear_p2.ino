        break;
      case 10:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, error);
        break;
    }
}
 
int PilotKeypadCheck() {
  if(calculateFace() < 0 && swActivated == 0) { // WAIT TO MAKE SURE DIP SWITCH IS READY BEFORE CONINUING
     return -1;
    }
 
  if(finald == 1) {  // final descent has begun - no more keypad input
      return -1;
  }
 
  char customKey = myAwesomePad.getKey();
 
  if (customKey == 'A') { // if correct key input was pressed
     wasPressed = 1;
    Serial.println(customKey);
    return 0;
  }
 
  else if (wasPressed == 1) {
    displayBitmap = 6;
  }
 
  return -1;
}
 
void advanceCountDown() {
  finald = 1;
  Altitude-- ; // reduce by 1 meter
 
  if(Altitude <= 0 && System_Error == 0){
    digitalWrite(LandingIndicator, HIGH);
    // picture loop  
    My_u8g_Panel.firstPage();  
    do {
      My_u8g_Panel.drawXBMP( 0, 0, 128, 64, partyFace);
    } while( My_u8g_Panel.nextPage() );
    displayBitmap = 1; // Party time!
    Altitude = 0;
    wasPressed = 0;
    //SUCCESSFULLY LANDED!!!!!!!!!!!!
  } 
  else if(Altitude > counter) {
    tone(buzzer, 60, 100);

    My_u8g_Panel.firstPage();  
    do {
      My_u8g_Panel.drawXBMP( 0, 0, 128, 64, increase);
    } while( My_u8g_Panel.nextPage() );
    displayBitmap = 3; // descending too quickly...
   }
   
  else if(Altitude < counter){
  tone(buzzer, 240, 100);
  My_u8g_Panel.firstPage();  
  do {
    My_u8g_Panel.drawXBMP( 0, 0, 128, 64, decrease);
  } while( My_u8g_Panel.nextPage() );
    displayBitmap = 4; // descending too slowly...
  }
 
  if((Altitude - counter) > 5 || (counter - Altitude) > 5) {
    Serial.println("TOO FAR OUT OF RANGE: RESTART!");
    displayBitmap = 10;  //exceeded landing tolerance!
    System_Error = 1; //ERROR FLAG - ABORT
    Ready = 0;  // start over
    finald = 0;
    Altitude = 30;
    counter = 30;
  }
//  Serial.println(Altitude);
}
 
 
int calculateFace(){
  if(swActivated == 1){
    return 0; //ALREADY COMPLETED DIP SWITCH SECTION
  }
 
  // READ THE DIP SWITCHES
  int  sw_one = digitalRead(LandingStartSW);
  int  sw_two = digitalRead(LandingApproachSW);
  int  sw_three = digitalRead(LandingTerminalSW);
 
  if(sw_one == 1 && sw_two == 0 && sw_three == 0) {
      displayBitmap = 7; // 1st throttle position
  }
  else if(sw_one == 0 && sw_two == 1 && sw_three == 0) {
      displayBitmap = 4; // go down to 1st throttle
  }
  else if(sw_one == 1 && sw_two == 1 && sw_three == 0) {
      displayBitmap = 8; // 2nd throttle position
  }
  else if(sw_one == 0 && sw_two == 0 && sw_three == 1) {
      displayBitmap = 2; // increased too fast!
  }
  else if(sw_one == 1 && sw_two == 0 && sw_three == 1) {
      displayBitmap = 10; // skipped 2nd phase!
  }
  else if(sw_one == 0 && sw_two == 1 && sw_three == 1) {
      displayBitmap = 10; // skipped 1st phase!
  }
  else if(sw_one == 1 && sw_two == 1 && sw_three == 1) {
      displayBitmap = 9; // good to go
      swActivated = 1;
      return 0;
  }
  else {
      displayBitmap = 0;
     
  }
return -1; // -1 returned unless switches are correct
}
 
// ************************************************
//  This is our ISR which has the job of responding to interrupt events
 
void updateEncoder(){
 
  if(wasPressed != 1) {  // not yet in terminal descent
    return ;
  }
  else {
    Ready = 1;
  }
 
  // Read the current state of encoder CLK
  currentStateCLK = digitalRead(CLK);
 
  // If last and current state of CLK are different, then pulse occurred
  // React to only 0->1 state change to avoid double counting
  if (currentStateCLK != lastStateCLK  && currentStateCLK == 1){
 
    // If the DT state is different than the CLK state then
    // the encoder is rotating CW so INCREASE counter by 1
    if (digitalRead(DT) != currentStateCLK) {
      counter ++;
      currentDir ="CW";
 
    } else {
      // Encoder is rotating CCW so DECREASE counter by 1
      counter --;
      currentDir ="CCW";
    }
 
    Serial.print("Direction: ");
    Serial.print(currentDir);
    Serial.print(" | Counter: ");
    Serial.print(counter);
    Serial.print(" | Altitude: ");
    Serial.println(Altitude);
  }
 
    // Remember last CLK state to use on next interrupt...
  lastStateCLK = currentStateCLK;
}
 
// ************************************************
void setup(void) {
  Serial.begin(9600);
 
  pinMode(LandingStartSW, INPUT);
  pinMode(LandingApproachSW, INPUT);
  pinMode(LandingTerminalSW, INPUT);
  pinMode(LandingIndicator, OUTPUT);
 
  // Set encoder pins as inputs
  pinMode(CLK, INPUT);
  pinMode(DT, INPUT);
 
  My_u8g_Panel.setColorIndex(1); // pixel on
  displayBitmap = 0;
 
  // Read the initial state of CLK
  lastStateCLK = digitalRead(CLK);
 
   // Call Interrupt Service Routine (ISR) updateEncoder() when any high/low change
  // is seen on A (CLK) interrupt  (pin 2), or B (DT) interrupt (pin 3)
 
  attachInterrupt(digitalPinToInterrupt(CLK), updateEncoder, CHANGE);
  attachInterrupt(digitalPinToInterrupt(DT), updateEncoder, CHANGE);
}
 
// ************************************************
void loop(void) {
  // picture loop  
  My_u8g_Panel.firstPage();  
  do {
    draw();
  } while( My_u8g_Panel.nextPage() );
 
  unsigned long currMillisec = millis();
 
  if (currMillisec - prevMillisec >= 1000) {
    // one second has elapsed since the countdown was last advanced...
    prevMillisec = currMillisec;
 
    if(Ready == 1) {
      advanceCountDown();
    }
  }
} 
