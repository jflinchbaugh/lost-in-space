// ************************************************
void setup(void) {
  Serial.begin(9600);
 
  pinMode(LandingStartSW, INPUT);
  pinMode(LandingApproachSW, INPUT);
  pinMode(LandingTerminalSW, INPUT);
  pinMode(LandingIndicator, OUTPUT);
 
  My_u8g_Panel.setColorIndex(1); // pixel on
  displayBitmap = 0;
}
 
// ************************************************
void loop(void) {
  char customKey = myAwesomePad.getKey();
 
 if (customKey) { // if a button is pressed
  Serial.println(customKey);
  digitalWrite(LandingIndicator, HIGH);
  delay(1000);
  digitalWrite(LandingIndicator, LOW);
 }
 
  // picture loop  
  My_u8g_Panel.firstPage();  
  do {
    draw();
  } while( My_u8g_Panel.nextPage() );
 
 
}
 
// ************************************************
// OLED Display output
void draw(void) {
  // graphic commands to redraw the complete screen should be placed here  
  My_u8g_Panel.setFont(u8g_font_unifont);
 
  calculateFace();
 
  switch (displayBitmap) {
      case 0:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, defaultFace);
        break;
      case 1:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, partyFace);
        break;
      case 2:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, yikesFace);
        break;
      case 3:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, increase);
        break;
      case 4:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, decrease);
        break;
      case 5:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, checkmark);
        break;
      case 6:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, threeCheck);
        break;
      case 7:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, lowThrottle);
        break;
      case 8:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, optimalThrottle);
        break;
      case 9:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, highThrottle);
        break;
      case 10:
        My_u8g_Panel.drawXBMP( 0, 0, 128, 64, error);
        break;
    }
   
}
 
void calculateFace(){
  // READ THE DIP SWITCH
  int  sw_one = digitalRead(LandingStartSW);
  int  sw_two = digitalRead(LandingApproachSW);
  int  sw_three = digitalRead(LandingTerminalSW);
 
  if(sw_one == 1 && sw_two == 0 && sw_three == 0) {
      displayBitmap = 1;
  }
  else if(sw_one == 0 && sw_two == 1 && sw_three == 0) {
      displayBitmap = 2;
  }
  else if(sw_one == 1 && sw_two == 1 && sw_three == 0) {
      displayBitmap = 3;
  }
  else if(sw_one == 0 && sw_two == 0 && sw_three == 1) {
      displayBitmap = 4;
  }
  else if(sw_one == 1 && sw_two == 0 && sw_three == 1) {
      displayBitmap = 5;
  }
  else if(sw_one == 0 && sw_two == 1 && sw_three == 1) {
      displayBitmap = 6;
  }
  else if(sw_one == 1 && sw_two == 1 && sw_three == 1) {
      displayBitmap = 7;
  }
  else {
      displayBitmap = 0;
  }
 
}
